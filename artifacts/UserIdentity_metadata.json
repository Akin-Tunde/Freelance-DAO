				"createProfile(string,string,bytes)": {
					"details": "A user can only create one profile. Usernames must be unique.",
					"params": {
						"_ipfsPortfolioHash": "An IPFS CID pointing to detailed portfolio information.",
						"_skills": "A comma-separated string of skills.",
						"_username": "A unique name for the profile."
