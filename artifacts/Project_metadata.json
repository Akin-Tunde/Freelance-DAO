		"devdoc": {
			"details": "Represents a single freelance job on the platform. This contract manages the project's lifecycle from proposal submissions to hiring, review, and completion or dispute. Each project is a self-contained state machine.",
			"kind": "dev",
			"methods": {
				"acceptWork()": {
					"details": "This is the final step in a successful project, triggering the Escrow contract."
				},
				"hireFreelancer(address)": {
					"details": "This action creates a new Escrow contract to secure the project funds.",
					"params": {
						"_freelancer": "The address of the freelancer to hire."
					}
				},
				"raiseDispute()": {
					"details": "This freezes the funds in Escrow and transitions the project to the InDispute state."
				},
				"submitProposal(string,uint256)": {
					"params": {
						"_details": "A string describing the freelancer's plan for the project.",
						"_proposedCost": "The freelancer's bid for the project."
					}
				}
			},
			"title": "Project",
			"version": 1
		},
		"userdoc": {
			"kind": "user",
			"methods": {
				"acceptWork()": {
					"notice": "Allows the project owner to accept the submitted work and release payment."
				},
				"hireFreelancer(address)": {
					"notice": "Allows the project owner to hire a freelancer who has submitted a proposal."
				},
				"raiseDispute()": {
					"notice": "Allows either the owner or the freelancer to raise a dispute."
				},
				"submitProposal(string,uint256)": {
					"notice": "Allows any user to submit a proposal for this project."
				},
				"submitWork()": {
					"notice": "Allows the hired freelancer to signal that the work is ready for review."
